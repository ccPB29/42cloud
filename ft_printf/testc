#include <stdio.h>    // 用于标准 printf
#include "ft_printf.h" // 替换为你的 ft_printf 头文件路径
#include <limits.h>

int main(void) {
    int ft_len, std_len;

    ft_len = ft_printf("%p", (void *)-14523);
    std_len = printf("%p", (void *)-14523);
    printf("ft_len = %d, std_len = %d\n\n", ft_len, std_len);

// " %p %p", 0, 0

}